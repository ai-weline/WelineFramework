<?xml version="1.0"?>
<config xmlns:xs="http://www.w3.org/2001/XMLSchema-instance"
        xs:noNamespaceSchemaLocation="urn:Weline_Framework::Event/etc/xsd/event.xsd"
        xmlns="urn:Weline_Framework::Event/etc/xsd/event.xsd">
    <event name="Framework_Acl::dispatch">
        <observer name="Weline_Acl::dispatch" instance="Weline\Acl\Observer\AclRegisterObserver" disabled="false" shared="true"/>
    </event>
    <!--控制器注解属性-->
    <event name="Weline_Module::controller_class_attributes">
        <observer name="Weline_Acl::controller_class_attributes" instance="Weline\Acl\Observer\ControllerClassAttribute" disabled="false"
                  shared="true"/>
    </event>
    <!--控制器方法注解属性-->
    <event name="Weline_Module::controller_method_attributes">
        <observer name="Weline_Acl::controller_method_attributes" instance="Weline\Acl\Observer\ControllerMethodAttributes" disabled="false"
                  shared="true"/>
    </event>
</config>