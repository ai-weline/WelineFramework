<?xml version="1.0"?>
<config xmlns:xs="http://www.w3.org/2001/XMLSchema-instance"
        xs:noNamespaceSchemaLocation="urn:Weline_Framework::Event/etc/xsd/event.xsd"
        xmlns="urn:Weline_Framework::Event/etc/xsd/event.xsd">
    <event name="Framework_Module::module_upgrade">
        <observer name="Weline_Frontend::menu_upgrade" instance="Weline\UrlManager\Observer\UrlUpdate" disabled="false" shared="true"/>
    </event>
    <event name="Weline_Framework_Router::router_start">
        <observer name="Weline_UrlManager::router_rewrite" instance="Weline\UrlManager\Observer\RouterRewrite" disabled="false" shared="true"/>
    </event>
</config>